pipeline{
    agent any
    stages{
        
        stage('Git Clone'){
            steps{
                git 'https://github.com/MHbappy/sampleProjectJenkins.git'
            }
        }
        
        stage('Package'){
            steps{
                sh 'mvn package'
            }
        }
        
        stage('Docker build'){
            steps{
                sh 'docker build -t bappy/jenkins-hello-world:latest .'
            }
        }
       
        
        
    }
}
